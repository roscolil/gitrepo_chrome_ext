{
  "manifest_version": 2,

  "name": "GitHub API Plugin",
  "description": "This extension is a user friendly plugin to access GitHub repos",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "page": "index.html"
  },
  "background": {
      "scripts": [
        "git.js"
      ]
    },
  "permissions": [
   "tabs"
   ]
}
